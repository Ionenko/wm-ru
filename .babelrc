{
    "presets": [
        [
            "@babel/preset-env",
            {
                "useBuiltIns": "usage",
                "targets": {
                    "browsers": [
                        "> 1%",
                        "last 2 versions",
                        "ie >= 9"
                    ]
                },
                "corejs": 3
            }
        ]
    ],
    "plugins": [
        ["@babel/plugin-transform-arrow-functions", { "spec": true }]
    ]
}